<Page loaded="pageLoaded" xmlns:lv="nativescript-telerik-ui/listview" xmlns="http://www.nativescript.org/tns.xsd">
    <lv:RadListView items="{{ eventsList }}" itemTap="{{ onEventTap }}" >
        <lv:RadListView.itemTemplate>
            <GridLayout>
                <Label col="0" text="{{ id }}" />
                <Label col="1" text="{{ title }}" />
            </GridLayout>
        </lv:RadListView.itemTemplate>
    </lv:RadListView>
</Page>